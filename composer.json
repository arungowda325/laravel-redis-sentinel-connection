{
    "name": "arungowda325/laravel-redis-sentinel-connection",
    "description": "Redis Sentinel connection for Laravel and Lumen.",
    "keywords": ["redis", "sentinel", "laravel", "lumen"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Phaneesh",
            "email": "phaneesh.hegde@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "illuminate/cache": ">=5.4",
        "illuminate/contracts": ">=5.4",
        "illuminate/queue": ">=5.4",
        "illuminate/redis": ">=5.4",
        "illuminate/session": ">=5.4",
        "illuminate/support": ">=5.4",
        "monospice/spicy-identifiers": ">=0.1",
        "predis/predis": ">=1.1"
    },
    "require-dev": {
        "laravel/framework": ">=5.4",
        "laravel/horizon": ">=1.0",
        "laravel/lumen-framework": ">=5.4",
        "mockery/mockery": ">=1.3",
        "phpunit/phpunit": ">=5.0"
    },
    "autoload": {
        "psr-4": {
            "Wolverine\\LaravelRedisSentinel\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Wolverine\\LaravelRedisSentinel\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Wolverine\\LaravelRedisSentinel\\RedisSentinelServiceProvider"
            ]
        }
    }
}
